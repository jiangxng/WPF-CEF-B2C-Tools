<UserControl x:Class="WPF_CEF_B2C_Tools.Components.XingWebBrowser"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPF_CEF_B2C_Tools.Components"
             mc:Ignorable="d" 
             xmlns:cefWpf="clr-namespace:CefSharp.Wpf;assembly=CefSharp.Wpf"
        xmlns:cefCore="clr-namespace:CefSharp;assembly=CefSharp.Core"
             d:DesignHeight="450" d:DesignWidth="800" Initialized="UserControl_Initialized">
    <Grid>
        <DockPanel LastChildFill="True">
            <TextBox DockPanel.Dock="Top" Name="txtAddrees" Text="" TextWrapping="Wrap" HorizontalAlignment="Stretch"/>
            <cefWpf:ChromiumWebBrowser Name="browser" Address="" ></cefWpf:ChromiumWebBrowser>
        </DockPanel>
        <TextBlock x:Name="logPanel" Text="" TextWrapping="Wrap" Width="671" HorizontalAlignment="Left" VerticalAlignment="Top" Height="120" Background="#66000000" Foreground="#FFFBFFAE" Margin="37,71,0,0" Focusable="True" AllowDrop="True"/>
    </Grid>
</UserControl>
